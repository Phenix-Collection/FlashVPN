<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >



    <item android:id="@android:id/secondaryProgress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="2.55"
                android:shape="ring"
                android:useLevel="true"
                android:thicknessRatio="9.23">

                <gradient
                    android:startColor="#6e0ebfd6"
                    android:endColor="#6e0ebfd6"
                    android:centerColor="#6e0ebfd6"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="2.55"
                android:shape="ring"
                android:useLevel="true"
                android:thicknessRatio="9.23">
                <gradient
                    android:startColor="#0ebfd6"
                    android:endColor="#0ebfd6"
                    android:centerColor="#0ebfd6"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
</layer-list>